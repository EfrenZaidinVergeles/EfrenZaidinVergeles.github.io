{
  "name": "app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=dev run-p serve:*",
    "serve:eleventy": "eleventy --serve --incremental",
    "serve:tailwind": "tailwindcss -i ./src/static/styles/index.css -o ./src/static/styles/styles.css --watch",
    "gulp-serve": "gulp default",
    "build": "cross-env NODE_ENV=prod run-s build:*",
    "build:eleventy": "eleventy",
    "build:tailwind": "tailwindcss -i ./src/static/styles/index.css -o ./src/static/styles/styles.css --minify",
    "gulp-build": "gulp imagemin"
  },
  "repository": "https://github.com/EfrenZaidinVergeles/EfrenZaidinVergeles.github.io.git",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@tailwindcss/typography": "^0.5.2",
    "alpinejs": "^3.10.2",
    "cross-env": "^7.0.3",
    "daisyui": "^2.15.1",
    "gulp": "^4.0.2",
    "gulp-imagemin": "7.1.0",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.24"
  }
}
